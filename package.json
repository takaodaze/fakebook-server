{
  "name": "fakebook",
  "version": "1.0.0",
  "description": "## 開発の目的 ### 学習 - TS・React のアウトプット - nodeJS のアウトプット - ついでにTS・webpack についての学習",
  "scripts": {
    "server:start": "npm --prefix ./server start",
    "server:start-qdev": "npm --prefix run start-dev",
    "server:test": "npm --prefix ./server test",
    "server:lint": "npm --prefix ./server run lint",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takaodaze/fakebook.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/takaodaze/fakebook/issues"
  },
  "homepage": "https://github.com/takaodaze/fakebook#readme",
  "lint-staged": {
    "server/src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write"
    ],
    "*.{js,css,md}": "prettier --write"
  },
  "devDependencies": {
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  }
}
